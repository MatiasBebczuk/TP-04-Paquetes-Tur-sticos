@{
    ViewData["Title"] = "Index";
    int i = 0;
}

<div>
    <h1>Index</h1>
    <p>Bienvenidos al sitio web de @ViewBag.NombreProyecto. Pagina con paquetes turisticos de primera calidad.</p>
</div>
@if(ViewBag.ListaPaquetes != null){
    foreach(KeyValuePair<string, Paquete> x in ViewBag.ListaPaquetes){
    i++;
    <p>Paquete numero @i</p>
    <li>Destino: @x.Key</li>
    <li>Hotel: @x.Value.Hotel </li>
    <li>Aereo: @x.Value.Aereo </li>
    <li>Excursion: @x.Value.Excursion </li>
    }
}


<a href='@Url.Action("SelectPaquete", "Home")'>Crear Paquete</a>